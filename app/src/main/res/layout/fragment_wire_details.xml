<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TabLayout.WireDetails">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="13dp"
            android:text="Kredsdetaljer"
            android:textSize="32sp"
            app:layout_constraintEnd_toStartOf="@+id/button3"
            app:layout_constraintHorizontal_bias="0.50"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button3"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="12dp"
            android:text="add"
            app:layout_constraintEnd_toStartOf="@+id/button2"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button2"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="18dp"
            android:text="remove"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TableLayout
            android:id="@+id/Kredsdetalje_Tabel1"
            android:layout_width="412dp"
            android:layout_height="wrap_content"
            android:padding="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.142">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/Group_Value"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:text="Gruppe"
                    android:textSize="27sp" />

                <TextView
                    android:id="@+id/OB_Value"
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:text="OB (Ln)"
                    android:textSize="27sp" />

                <TextView
                    android:id="@+id/Karakter_Value"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    android:text="Karakterastik"
                    android:textSize="27sp" />

            </TableRow>

        </TableLayout>

        <TableLayout
            android:id="@+id/Kredsdetalje_Tabel1_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Kredsdetalje_Tabel1">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0">

                    <EditText
                        android:id="@+id/Group_Value1"
                        android:layout_width="110dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/OB_Value1"
                        android:layout_width="110dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/Karakter_Value1"
                        android:layout_width="172dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                </TableRow>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <EditText
                        android:id="@+id/Group_Value2"
                        android:layout_width="110dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/OB_Value2"
                        android:layout_width="110dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/Karakter_Value2"
                        android:layout_width="172dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                </TableRow>

            </TableRow>

        </TableLayout>

        <TableLayout
            android:id="@+id/Kredsdetalje_Tabel2"
            android:layout_width="412dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Kredsdetalje_Tabel1_value">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/Crosscut_Value"
                    android:layout_width="115dp"
                    android:layout_height="match_parent"
                    android:text="mm2"
                    android:textSize="27sp" />

                <TextView
                    android:id="@+id/MaxOB_Value"
                    android:layout_width="175dp"
                    android:layout_height="match_parent"
                    android:text="Maks.OB (Ln)"
                    android:textSize="27sp" />

                <TextView
                    android:id="@+id/Isolation_Value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Isolation"
                    android:textSize="27sp" />

            </TableRow>

        </TableLayout>

        <TableLayout
            android:id="@+id/Kredsdetalje_Tabel2_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Kredsdetalje_Tabel2">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <EditText
                        android:id="@+id/Crosscut_Value1"
                        android:layout_width="115dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/MaxOB_Value1"
                        android:layout_width="177dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/Isolation_Value1"
                        android:layout_width="101dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                </TableRow>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <EditText
                        android:id="@+id/Crosscut_Value2"
                        android:layout_width="115dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/MaxOB_Value2"
                        android:layout_width="177dp"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                    <EditText
                        android:id="@+id/Isolation_Value2"
                        android:layout_width="101dp"
                        android:layout_height="match_parent"
                        android:ems="10"
                        android:hint="value"
                        android:inputType="textPersonName" />

                </TableRow>

            </TableRow>

        </TableLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>